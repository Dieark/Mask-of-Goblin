{
  "name": "mask-of-goblin",
  "version": "2.0.0",
  "author": "Bjorn Lim <mail@bjornl.im>",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "serve": "serve dist"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "buefy": "^0.7.0",
    "bulmaswatch": "^0.7.2",
    "svg-innerhtml": "^1.1.0",
    "vue": "^2.5.17",
    "vue-awesome": "^3.2.0",
    "vue-meta": "^1.5.5",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-i18n": "^1.10.5",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.4",
    "@vue/cli-service": "^3.1.3",
    "@vue/eslint-config-prettier": "^4.0.0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "serve": "^10.0.2",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/prettier"
    ],
    "rules": {
      "vue/attribute-hyphenation": [
        "error",
        "always"
      ],
      "vue/html-indent": [
        "error",
        2
      ],
      "vue/html-quotes": [
        "error",
        "double"
      ],
      "vue/html-closing-bracket-spacing": "error"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
